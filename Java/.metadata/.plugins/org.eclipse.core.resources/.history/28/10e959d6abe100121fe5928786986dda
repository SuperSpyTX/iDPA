package se.krau.asciiani;

import java.io.IOException;

public class AsciiAnimation {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		for (;;) {
			Renderer.testRender();
			try {
				(new jline.ConsoleReader()).clearScreen();
			} catch (IOException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
				break;
			}
		}
	}

}
